{
  "fonts": {
    "imingcp": {
      "name": "I.MingCP",
      "displayName": "I.MingCP",
      "source": {
        "type": "github-release",
        "owner": "ichitenfont",
        "repo": "I.Ming",
        "filePattern": "I.MingCP-{version}.ttf",
        "url": "https://github.com/ichitenfont/I.Ming"
      },
      "license": {
        "type": "IPA Font License Agreement v1.0",
        "url": "https://github.com/ichitenfont/I.Ming/blob/master/LICENSE.md"
      },
      "weight": 400,
      "style": "normal",
      "subset": {
        "type": "size-based-chunks",
        "strategy": "chinese-frequency",
        "chunkSizes": [80, 150, 150, 150, 150, 200, 200, 200, 250, 250, 250],
        "maxChunks": 30,
        "ensureCompleteCoverage": true,
        "priorityData": "traditional-chinese-frequency"
      },
      "output": {
        "formats": ["woff2"],
        "filenamePattern": "imingcp-chunk-{index}"
      }
    },
    "lxgwwenkaitc": {
      "name": "LXGWWenKaiTC",
      "displayName": "LXGW WenKai TC",
      "source": {
        "type": "github-release",
        "owner": "lxgw",
        "repo": "LxgwWenkaiTC",
        "filePattern": "LXGWWenKaiTC-Light.ttf",
        "url": "https://github.com/lxgw/LxgwWenkaiTC"
      },
      "license": {
        "type": "SIL Open Font License 1.1",
        "url": "https://github.com/lxgw/LxgwWenkaiTC/blob/main/OFL.txt"
      },
      "weight": 300,
      "style": "normal",
      "subset": {
        "type": "size-based-chunks",
        "strategy": "chinese-frequency",
        "chunkSizes": [80, 150, 150, 150, 150, 200, 200, 200, 250, 250, 250],
        "maxChunks": 30,
        "ensureCompleteCoverage": true,
        "priorityData": "traditional-chinese-frequency"
      },
      "output": {
        "formats": ["woff2"],
        "filenamePattern": "lxgwwenkaitc-chunk-{index}"
      }
    },
    "amstelvar": {
      "name": "Amstelvar",
      "displayName": "Amstelvar",
      "source": {
        "type": "github-repo",
        "owner": "googlefonts",
        "repo": "amstelvar",
        "url": "https://github.com/googlefonts/amstelvar",
        "files": [
          {
            "path": "fonts/Amstelvar-Roman[GRAD,XOPQ,XTRA,YOPQ,YTAS,YTDE,YTFI,YTLC,YTUC,wdth,wght,opsz].ttf",
            "style": "roman"
          },
          {
            "path": "fonts/Amstelvar-Italic[GRAD,YOPQ,YTAS,YTDE,YTFI,YTLC,YTUC,wdth,wght,opsz].ttf",
            "style": "italic"
          }
        ]
      },
      "license": {
        "type": "SIL Open Font License 1.1",
        "url": "https://github.com/googlefonts/amstelvar/blob/main/OFL.txt"
      },
      "type": "variable",
      "styles": ["roman", "italic"],
      "subset": {
        "type": "size-based-chunks",
        "strategy": "latin-frequency",
        "chunkSizes": [80, 150, 200, 250],
        "maxChunks": 8,
        "ensureCompleteCoverage": true,
        "priorityData": "latin-frequency"
      },
      "output": {
        "formats": ["woff2"],
        "filenamePattern": "amstelvar-{style}-chunk-{index}"
      },
      "variableAxes": ["GRAD", "XOPQ", "XTRA", "YOPQ", "YTAS", "YTDE", "YTFI", "YTLC", "YTUC", "wdth", "wght", "opsz"]
    }
  },
  "build": {
    "outputDir": "build",
    "fontsDir": "fonts",
    "cssDir": "css"
  },
  "subsetting": {
    "formats": ["woff2"],
    "compression": "brotli",
    "hinting": true,
    "desubroutinize": true
  }
}
