{
  "name": "web-font-auto-subsetting",
  "version": "1.0.0",
  "description": "Automated web font subsetting and deployment workflow",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "check-versions": "node src/versionChecker.js",
    "download-fonts": "node scripts/download-fonts.js",
    "subset-fonts": "node src/fontSubset.js",
    "generate-css": "node scripts/generate-css.js",
    "generate-license": "node scripts/generate-license.js",
    "build": "pnpm run download-fonts && pnpm run subset-fonts && pnpm run generate-css && pnpm run generate-license",
    "clean": "./clean-cache.sh",
    "clean:build": "./clean-cache.sh --build",
    "clean:deps": "./clean-cache.sh --deps",
    "clean:git": "./clean-cache.sh --git",
    "clean:all": "./clean-cache.sh --all",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "fonts",
    "subsetting",
    "web-fonts",
    "automation",
    "github-actions"
  ],
  "author": "GitHub Copilot",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "cssnano": "^7.0.7",
    "eslint": "^9.29.0",
    "postcss": "^8.5.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
