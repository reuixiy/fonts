  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "node-fetch": "^3.3.2",
    "fs-extra": "^11.2.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
